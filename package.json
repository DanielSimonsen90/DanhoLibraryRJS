{
  "name": "danholibraryrjs",
  "description": "DanhoLibrary for React.js",
  "author": "Daniel Simonsen",
  "version": "1.0.4",
  "type": "commonjs",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "license": "ISC",
  "scripts": {
    "build-ts": "npx tsc --project tsconfig.json",
    "build-sass": "npx node-sass -w src/styles/index.scss dist/styles/index.css",
    "build-sass-min": "npx node-sass -w src/styles/index.scss dist/styles/index.min.css --output-style compressed",
    "build": "npm run build-ts && npm run build-sass && npm run build-sass-min"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DanielSimonsen90/DanhoLibraryRJS.git"
  },
  "dependencies": {
    "@types/lodash": "^4.14.178",
    "copy-to-clipboard": "^3.3.1",
    "danholibraryjs": "^1.6.0",
    "js-cookie": "^3.0.1",
    "node-sass": "^6.0.1",
    "react-fontawesome": "^1.7.1",
    "react-router-dom": "^5.2.0",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.0",
    "@types/node-sass": "^4.11.2",
    "@types/react": "^17.0.42",
    "@types/react-dom": "^17.0.10",
    "@types/react-fontawesome": "^1.6.5",
    "@types/react-router-dom": "^5.3.2"
  }
}
