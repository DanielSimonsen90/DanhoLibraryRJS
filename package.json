{
  "name": "danholibraryrjs",
  "version": "1.3.4",
  "description": "DanhoLibrary for React.js",
  "author": "DanielSimonsen90",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build-ts": "npx tsc --project tsconfig.json",
    "build-sass": "npx node-sass -w src/styles/index.scss dist/styles/index.css",
    "build-sass-min": "npx node-sass -w src/styles/index.scss dist/styles/index.min.css --output-style compressed",
    "build": "npm run build-ts && npm run build-sass && npm run build-sass-min",
    "login-github": "npm login --scope=@DanielSimonsen90 --registry=https://npm.pkg.github.com",
    "login-npm": "npm login --scope@danhosaur --registry=https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielSimonsen90/DanhoLibraryRJS.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "library",
    "react"
  ],
  "license": "ISC",
  "types": "./dist\\index.d.ts",
  "bugs": {
    "url": "https://github.com/DanielSimonsen90/DanhoLibraryRJS/issues"
  },
  "homepage": "https://github.com/DanielSimonsen90/DanhoLibraryRJS#readme",
  "dependencies": {
    "copy-to-clipboard": "^3.3.2",
    "danholibraryjs": "github:danielsimonsen90/danholibraryjs",
    "js-cookie": "^3.0.1",
    "node-sass": "^7.0.1",
    "react-fontawesome": "^1.7.1",
    "react-router-dom": "^6.3.0",
    "typescript": "^4.8.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.2",
    "@types/lodash": "^4.14.184",
    "@types/node-sass": "^4.11.3",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/react-fontawesome": "^1.6.5",
    "@types/react-router-dom": "^5.3.3"
  }
}
